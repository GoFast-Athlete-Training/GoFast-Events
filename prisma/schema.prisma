generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model VolunteerInput {
  id        String   @id @default(cuid())
  name      String
  email     String
  role      String
  note      String?
  createdAt DateTime @default(now())
}
